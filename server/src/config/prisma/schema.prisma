generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int    @id @default(autoincrement())
  first_name    String
  last_name     String
  email         String @unique
  password_hash String

  created_at DateTime @default(now())
  updatat_at DateTime @default(now())
}
